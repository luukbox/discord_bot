{
  "name": "luukbox",
  "version": "1.0.0",
  "description": "Discord Music Bot because the others suck",
  "main": "main.ts",
  "scripts": {
    "build": "tsc",
    "start": "build && node ./dist/main.js",
    "pm2": "tsc && pm2 start ./dist/main.js --name luukbox --exp-backoff-restart-delay=100",
    "dev": "ts-node --watch ./src/**/*.ts ./src/main.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luukbox/discord_bot.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/luukbox/discord_bot/issues"
  },
  "homepage": "https://github.com/luukbox/discord_bot#readme",
  "dependencies": {
    "@types/node": "^14.14.31",
    "discord.js": "^11.4.2",
    "dotenv": "^6.2.0",
    "opusscript": "^0.0.8",
    "simple-youtube-api": "^5.1.1",
    "spotify-web-api-node": "^5.0.2",
    "ytdl-core": "^4.5.0"
  },
  "devDependencies": {
    "@types/spotify-web-api-node": "^5.0.0",
    "pm2": "^4.5.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^4.2.2"
  }
}
